		cam_core_en_default: cam_core_en_default {
			mux {
				pins =  "gpio39","gpio88";
				function = "gpio";
			};

			config {
				pins ="gpio39","gpio88";
				bias-pull-down; /* PULL DOWN */
				drive-strength = <2>;
				output-low;
			};
		};

		cam_core_en_sleep: cam_core_en_sleep {
